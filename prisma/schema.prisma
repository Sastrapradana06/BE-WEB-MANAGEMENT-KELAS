// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int     @id @default(autoincrement())
  username      String
  email         String  @unique
  password      String  
}

model Siswa {
  id                  Int     @id @default(autoincrement())
  image               String
  username            String
  tanggal_lahir       String
  email               String  @unique
  notel               String
  jabatan             String
  nama_ortu           String
  alamat              String  
  jekel               String  
}




